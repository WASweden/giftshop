<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
<meta name="viewport" content="width=device-width">
<title><%= content_for?(:title) ? yield(:title) + ' | WaterAid Gåvoshop' : 'WaterAid Gåvoshop' %></title>
<link rel="canonical" href="<%= request.original_url %>">

<meta name="description" content="<%= content_for?(:description) ? yield(:description) : 'Beställ ett gåvobevis och ge bort rent vatten i present. En gåva som verkligen förändrar liv.' %>">
<link rel="shortcut icon" href="/favicon.ico" type="image/x-icon" />

<%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag "head" %>
  <!--[if lt IE 9]>
    <%= javascript_include_tag "ie" %>
  <![endif]-->
<%= csrf_meta_tags %>

